# ============================================
# Trencher Bundler - Configuration
# ============================================
# Copy this file to .env and fill in your values:
#   cp .env.example .env
# ============================================

# ============================================
# REQUIRED - Must configure before use
# ============================================

# Your main funding wallet private key (base58 encoded)
PRIVATE_KEY=

# Solana RPC endpoint
# HIGHLY RECOMMENDED: Use Helius (free tier), QuickNode, or Alchemy
# Free tier works fine - avoids rate limits and failed launches
# Get free Helius RPC: https://helius.dev
# Public RPC (NOT recommended - will cause failures): https://api.mainnet-beta.solana.com
RPC_ENDPOINT=

# WebSocket endpoint (optional - for real-time tracking)
RPC_WEBSOCKET_ENDPOINT=

# ============================================
# TOKEN INFO
# ============================================

TOKEN_NAME=Trencher Bundler
TOKEN_SYMBOL=TRENCHER
DESCRIPTION=The ONLY Pump.fun bundler with a full web dashboard. Launch tokens, manage wallets, auto-sell, and track P&L â€” all from one page.
FILE=./image/1.png

# Social links (optional)
TWITTER=https://x.com/trencherbundler_x
TELEGRAM=https://t.me/dogtoshi_x
WEBSITE=https://trencherbundler.fun

# ============================================
# BUNDLE SETTINGS
# ============================================

BUNDLE_WALLET_COUNT=5
BUNDLE_SWAP_AMOUNTS=0.5,0.5,0.5,0.5,0.5
SWAP_AMOUNT=0.5

# ============================================
# HOLDER WALLET SETTINGS
# ============================================

HOLDER_WALLET_COUNT=1
HOLDER_WALLET_AMOUNT=0.5
HOLDER_SWAP_AMOUNTS=0.5
HOLDER_WALLET_PRIORITY_FEE=100000

# ============================================
# DEV/CREATOR WALLET
# ============================================

BUYER_AMOUNT=1
USE_FUNDING_AS_BUYER=false

# ============================================
# JITO SETTINGS
# ============================================

JITO_FEE=0.001
MINIMUM_JITO_TIP=0.0001
USE_JITO_FOR_SELLS=false
JITO_SELL_TIP=0.0035
RAPID_SELL_PRIORITY_FEE=7000000

# ============================================
# AUTO ACTIONS (After Launch)
# ============================================

AUTO_RAPID_SELL=false
AUTO_GATHER=true
AUTO_SELL_50_PERCENT=false
AUTO_SELL_STAGED=false
AUTO_COLLECT_FEES=true

# ============================================
# PRIVACY SETTINGS
# ============================================

DIRECT_SEND_MODE=true
USE_MIXING_WALLETS=false
CREATE_FRESH_MIXING_WALLETS=true
USE_MULTI_INTERMEDIARY_SYSTEM=false
NUM_INTERMEDIARY_HOPS=2
BUNDLE_INTERMEDIARY_HOPS=2
HOLDER_INTERMEDIARY_HOPS=2

# ============================================
# PRIORITY FEES (lamports)
# ============================================

PRIORITY_FEE_LAMPORTS_HIGH=500000
PRIORITY_FEE_LAMPORTS_MEDIUM=100000
PRIORITY_FEE_LAMPORTS_LOW=25000

# ============================================
# VANITY ADDRESSES
# ============================================
# Set to true to generate addresses ending with "pump" when no pump addresses in pool
# Set to false to use regular random addresses (recommended for most users)

VANITY_MODE=false

# ============================================
# SIMULATION MODE
# ============================================

SIMULATE_ONLY=false

# ============================================
# OPTIONAL: QuickNode Lil Jito
# ============================================

LIL_JIT_ENDPOINT=
LIL_JIT_WEBSOCKET_ENDPOINT=
LIL_JIT_MODE=false
